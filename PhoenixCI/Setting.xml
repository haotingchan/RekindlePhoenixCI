<?xml version="1.0" encoding="utf-8" ?>
<Setting>
  <System>PhoenixCI</System>

  <Database>
    <!--交易資訊系統資料庫-->
    <CiUserAp>
      <ConnectionName>CIN_USER_AP</ConnectionName>
      <InitialCatalog>CI</InitialCatalog>
    </CiUserAp>

    <!--交易資訊系統資料庫-CI帳號-->
    <Ci>
      <ConnectionName>CIN_CI</ConnectionName>
      <InitialCatalog>CI</InitialCatalog>
    </Ci>

    <!--交易資訊系統資料庫-期貨日盤-->
    <CiFut>
      <ConnectionName>CIN_FUT</ConnectionName>
      <InitialCatalog>CI</InitialCatalog>
    </CiFut>

    <!--交易資訊系統資料庫-選擇權日盤-->
    <CiOpt>
      <ConnectionName>CIN_OPT</ConnectionName>
      <InitialCatalog>CI</InitialCatalog>
    </CiOpt>

    <!--交易資訊系統資料庫-期貨夜盤-->
    <CiFutAH>
      <ConnectionName>CIN_FUT_AH</ConnectionName>
      <InitialCatalog>CI</InitialCatalog>
    </CiFutAH>

    <!--交易資訊系統資料庫-選擇權夜盤-->
    <CiOptAH>
      <ConnectionName>CIN_OPT_AH</ConnectionName>
      <InitialCatalog>CI</InitialCatalog>
    </CiOptAH>

    <!--交易資訊系統資料庫-監視-->
    <CiMonit>
      <ConnectionName>CIN_MONIT</ConnectionName>
      <InitialCatalog>CI</InitialCatalog>
    </CiMonit>

    <POSOnwer>
      <ConnectionName>POS_ONWER</ConnectionName>
      <InitialCatalog>192.168.88.51</InitialCatalog>
    </POSOnwer>
    
  </Database>

  <Log>
    <LogSeverity>Info</LogSeverity>
  </Log>

  <Uploads>

    <Item>
      <DisplayName>EXE</DisplayName>
      <ConnectionName>CIN_USER_AP</ConnectionName>
      <SystemName>PhoenixCI</SystemName>
      <Type>D</Type>
      <Filters>exe,dll,config,txt</Filters>
    </Item>
    <Item>
      <DisplayName>Template</DisplayName>
      <ConnectionName>CIN_USER_AP</ConnectionName>
      <SystemName>PhoenixCI</SystemName>
      <Type>X</Type>
      <Filters>xls,xlsx,xlsm,ifd,rtf</Filters>
    </Item>

  </Uploads>



  <ConnectionStrings>
    <!--測試環境區段-->
    <Connection>
      <Name>CIN_USER_AP</Name>
      <ConnectionString>Data Source=(DESCRIPTION=(ADDRESS_LIST=(ADDRESS=(PROTOCOL=TCP)(HOST=192.168.88.51)(PORT=1521)))(CONNECT_DATA=(SID=CI)));User Id=CI;Password=orcl;</ConnectionString>
      <ProviderName>Oracle.ManagedDataAccess.Client</ProviderName>
    </Connection>
    <Connection>
      <Name>CIN_CI</Name>
      <ConnectionString>Data Source=(DESCRIPTION=(ADDRESS_LIST=(ADDRESS=(PROTOCOL=TCP)(HOST=192.168.88.51)(PORT=1521)))(CONNECT_DATA=(SID=CI)));User Id=CI;Password=orcl;</ConnectionString>
      <ProviderName>Oracle.ManagedDataAccess.Client</ProviderName>
    </Connection>
    <Connection>
      <Name>CIN_FUT</Name>
      <ConnectionString>Data Source=(DESCRIPTION=(ADDRESS_LIST=(ADDRESS=(PROTOCOL=TCP)(HOST=192.168.88.51)(PORT=1521)))(CONNECT_DATA=(SID=CI)));User Id=CI;Password=orcl;</ConnectionString>
      <ProviderName>Oracle.ManagedDataAccess.Client</ProviderName>
    </Connection>
    <Connection>
      <Name>CIN_OPT</Name>
      <ConnectionString>Data Source=(DESCRIPTION=(ADDRESS_LIST=(ADDRESS=(PROTOCOL=TCP)(HOST=192.168.88.51)(PORT=1521)))(CONNECT_DATA=(SID=CI)));User Id=CI;Password=orcl;</ConnectionString>
      <ProviderName>Oracle.ManagedDataAccess.Client</ProviderName>
    </Connection>
    <Connection>
      <Name>CIN_FUT_AH</Name>
      <ConnectionString>Data Source=(DESCRIPTION=(ADDRESS_LIST=(ADDRESS=(PROTOCOL=TCP)(HOST=192.168.88.51)(PORT=1521)))(CONNECT_DATA=(SID=CI)));User Id=CI;Password=orcl;</ConnectionString>
      <ProviderName>Oracle.ManagedDataAccess.Client</ProviderName>
    </Connection>
    <Connection>
      <Name>CIN_OPT_AH</Name>
      <ConnectionString>Data Source=(DESCRIPTION=(ADDRESS_LIST=(ADDRESS=(PROTOCOL=TCP)(HOST=192.168.88.51)(PORT=1521)))(CONNECT_DATA=(SID=CI)));User Id=CI;Password=orcl;</ConnectionString>
      <ProviderName>Oracle.ManagedDataAccess.Client</ProviderName>
    </Connection>
    <Connection>
      <Name>CIN_MONIT</Name>
      <ConnectionString>Data Source=(DESCRIPTION=(ADDRESS_LIST=(ADDRESS=(PROTOCOL=TCP)(HOST=192.168.88.51)(PORT=1521)))(CONNECT_DATA=(SID=CI)));User Id=CI;Password=orcl;</ConnectionString>
      <ProviderName>Oracle.ManagedDataAccess.Client</ProviderName>
    </Connection>
    <!--正式環境區段-->
    <!--待填-->
  </ConnectionStrings>

  <!--下載程式用-->
  <Download>
    <ConnectionName>CIN_USER_AP</ConnectionName>
    <ProgramBeOpenedAfterDownload>PhoenixCI.exe</ProgramBeOpenedAfterDownload>
    <Type>D,X</Type>
  </Download>

</Setting>